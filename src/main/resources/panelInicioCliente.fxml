<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="488.0" prefWidth="341.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.bookyourstay.controlador.panelClienteControlador.PanelInicioClienteControlador">

   <AnchorPane prefHeight="488.0" prefWidth="321.0">
      <children>
         <Label layoutX="34.0" layoutY="4.0" text="Mi perfil" textFill="#4947ee">
            <font>
               <Font name="System Bold" size="43.0" />
            </font>
         </Label>
         <Label layoutX="121.0" layoutY="68.0" text="BookYourStayCo" textFill="#2946c6" />
         <Label layoutY="89.0" prefHeight="40.0" prefWidth="280.0" text="¿Qué deseas hacer?">
            <font>
               <Font name="System Bold" size="29.0" />
            </font>
         </Label>
         <Button fx:id="btnConfigurarCuenta" layoutX="9.0" layoutY="159.0" mnemonicParsing="false" onAction="#mostrarConfiguracion" prefHeight="39.0" prefWidth="183.0" text="Configurar cuenta">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Button fx:id="btnGestionarReservas" layoutX="9.0" layoutY="276.0" mnemonicParsing="false" onAction="#mostrarCrearReserva" prefHeight="39.0" prefWidth="183.0" text="Crear reserva">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Button fx:id="btnNuevaContrasenia" layoutX="9.0" layoutY="411.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="183.0" text="Nueva contraseña">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Button fx:id="btnRecargarBilletera" layoutX="9.0" layoutY="343.0" mnemonicParsing="false" onAction="#mostrarRecargarBilletera" prefHeight="39.0" prefWidth="183.0" text="Recargar billetera">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Button fx:id="btnGestionarReservas1" layoutX="9.0" layoutY="213.0" mnemonicParsing="false" onAction="#mostrarGestionReserva" prefHeight="39.0" prefWidth="183.0" text=" Gestionar reservas">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
      </children>
      <effect>
         <Blend />
      </effect>
   </AnchorPane>
   <StackPane fx:id="panelPrincipal" />
</HBox>
